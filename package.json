{
  "name": "nuxt-ts-gqless",
  "version": "1.0.0",
  "description": "Nuxt.js with gqless",
  "author": "Maarten Van Hoof",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
