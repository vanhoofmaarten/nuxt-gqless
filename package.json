{
  "name": "nuxt-ts-gqless",
  "version": "1.0.0",
  "description": "Nuxt.js with gqless",
  "author": "Maarten Van Hoof",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts -r dotenv/config",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "lint": "eslint --ext .ts,.vue --ignore-path .gitignore .",
    "generate": "nuxt generate",
    "gqless:generate": "gqless generate"
  },
  "lint-staged": {
    "*.{js,vue}": "npm run lint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": ""
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.0",
    "@nuxtjs/dotenv": "^1.4.0",
    "apollo-server-micro": "^2.11.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "gqless": "0.0.1-alpha.27",
    "graphql-pokeapi": "git+https://github.com/mazipan/graphql-pokeapi.git",
    "micro-route": "^2.5.0",
    "nuxt": "^2.0.0",
    "ts-node": "^8.7.0"
  },
  "devDependencies": {
    "@gqless/cli": "0.0.1-alpha.30",
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/stylelint-module": "^3.1.0",
    "@nuxtjs/tailwindcss": "^1.0.0",
    "@types/micro": "^7.3.3",
    "babel-eslint": "^10.0.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "stylelint": "^13.2.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-recess-order": "^2.0.4",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-prettier": "^1.1.2"
  }
